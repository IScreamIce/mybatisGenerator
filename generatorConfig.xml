<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

    <!--驱动包的位置-->
	<classPathEntry location="D:\Ggenerator\mysql-connector-java-5.1.34.jar" />

    <context id="mysqlTables" targetRuntime="MyBatis3">
        <!--数据源配置-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql:///videocopy?characterEncoding=utf-8"
                        userId="root"
                        password=""/>
        <!--指定生成的类型为java类型，避免数据库中number等类型字段 -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!--自动生成的实体的存放包路径 类 -->
        <javaModelGenerator targetPackage="com.kaige123.daomu.eureka_client_video_user.DAO.Bean"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!--自动生成的*Mapper.xml文件存放路径 -->
        <sqlMapGenerator targetPackage="com.kaige123.daomu.eureka_client_video_user.DAO.Mapper"
                         targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!--自动生成的*Mapper.java存放路径 接口 -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.kaige123.daomu.eureka_client_video_user.DAO.Mapper"
                             targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--table表格的配置
        tableName表格名称 domainObjectName实例名称 Example是否生成表达式-->
        <table tableName="banner" domainObjectName="Banner"
               enableCountByExample="true" enableUpdateByExample="true"
               enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true">
        </table>
		
		<table tableName="drawmony" domainObjectName="Drawmony"
               enableCountByExample="true" enableUpdateByExample="true"
               enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true">
